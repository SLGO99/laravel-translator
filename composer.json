{
    "name": "webo3/laravel-translator",
    "description": "This package add artisan commands to scan/export/import translations.",
    "license": "MIT",
    "homepage": "https://www.webo3.com",
    "authors": [
        {
            "name": "Hugues Joyal",
            "email": "hjoyal@webo3.com"
        }
    ],
    "require": {
        "php" : ">=7.0"
    },
    "autoload": {
        "psr-4": {
            "webO3\\Translator\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "webO3\\Translator\\Providers\\TranslatorServiceProvider"
            ]
        }
    }
}
